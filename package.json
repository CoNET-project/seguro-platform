		"react-intl": "^6.3.2",
		"react-redux": "^8.0.5",
		"react-scripts": "^5.0.1",
		"styled-components": "^5.3.9",
		"typescript": "^5.0.4",
		"wait-on": "^7.0.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"eslintConfig": {
		"overrides": [{
			"files": [
				"**/*.stories.*"
			],
			"rules": {
				"import/no-anonymous-default-export": "off"
			}
		}
	]}
}
