{
	"name": "@conet-project/seguro-platform",
	"version": "0.60.0",
	"author": "CoNET technology Inc.",
	"description": "The CoNET Platform",
	"license": "UNLICENSED",
	"files": [
		"./build"
	],
	"main": "./src/electron/electron.js",
	"scripts": {
		"start": "concurrently --kill-others --success=first 'yarn start:gateway' 'yarn _start:web' 'wait-on tcp:3000 && NODE_ENV=development yarn _start:desktop'",
		"start:storybook": "start-storybook -p 6006 -s public",
		"start:gateway": "yarn seguro-gateway --port 3001",
		"start:web": "concurrently --kill-others --success=first 'yarn _start:web'",
		"_start:web": "BROWSER=none react-scripts start",
		"__start:web": "BROWSER=none NODE_ENV=development node src/electron/start.js ",
		"_start:desktop": "electron ./src/electron/electron.js",
		"build:web": "react-scripts build",
		"lint": "echo 'no linter available'",
		"test": "react-scripts test",
		"clean": "rm -rf ./node_modules ./build ./dist",
		"build-storybook": "build-storybook -s public"
	},
	"dependencies": {
		"@conet.project/seguro-gateway": "0.18.5",
		"@conet.project/seguro-worker-lib": "0.13.7",
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@mui/icons-material": "^5.11.16",
		"@mui/material": "^5.11.16",
		"@react-icons/all-files": "^4.1.0",
		"@tippyjs/react": "^4.2.6",
		"dayjs": "^1.11.7",
		"framer-motion": "^10.11.2",
		"get-port": "^6.1.2",
		"react-draggable": "^4.4.5",
		"react-hot-toast": "^2.4.0",
		"react-loader-spinner": "^5.3.4",
		"react-lottie-player": "^1.5.4",
		"react-router-dom": "^6.10.0",
		"uuid": "^9.0.0",
		"web-vitals": "^3.3.1",
		"workbox-background-sync": "^6.5.4",
		"workbox-broadcast-update": "^6.5.4",
		"workbox-cacheable-response": "^6.5.4",
		"workbox-core": "^6.5.4",
		"workbox-expiration": "^6.5.4",
		"workbox-google-analytics": "^6.5.4",
		"workbox-navigation-preload": "^6.5.4",
		"workbox-precaching": "^6.5.4",
		"workbox-range-requests": "^6.5.4",
		"workbox-routing": "^6.5.4",
		"workbox-strategies": "^6.5.4",
		"workbox-streams": "^6.5.4"
	},
	"devDependencies": {
		"@reduxjs/toolkit": "^1.9.3",
		"@storybook/addon-actions": "^7.0.2",
		"@storybook/addon-essentials": "^7.0.2",
		"@storybook/addon-links": "^7.0.2",
		"@storybook/node-logger": "^7.0.2",
		"@storybook/preset-create-react-app": "^7.0.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"@storybook/react": "^7.0.2",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.5.0",
		"@types/node": "^18.15.11",
		"@types/react": "^18.0.33",
		"@types/react-dom": "^18.0.11",
		"@types/react-router-dom": "^5.3.3",
		"@types/styled-components": "^5.1.26",
		"@types/uuid": "^9.0.1",
		"babel-loader": "^9.1.2",
		"concurrently": "^8.0.1",
		"cors": "^2.8.5",
		"electron": "^24.0.0",
		"express": "^4.18.2",
		"modern-normalize": "^1.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
