{
  "name": "@conet-project/seguro-platform",
  "version": "0.55.5",
  "author": "Kloak Information Technologies",
  "description": "The Seguro Platform application.",
  "license": "UNLICENSED",
  "files": [
    "./build"
  ],
  "main": "./src/electron/electron.js",
  "scripts": {
    "start": "concurrently --kill-others --success=first 'yarn start:gateway' 'yarn _start:web' 'wait-on tcp:3000 && NODE_ENV=development yarn _start:desktop'",
    "start:storybook": "start-storybook -p 6006 -s public",
    "start:gateway": "yarn seguro-gateway --port 3001",
    "start:web": "concurrently --kill-others --success=first 'yarn _start:web' 'seguro-gateway --port 3001'",
    "_start:web": "BROWSER=none react-scripts start",
    "_start:desktop": "electron ./src/electron/electron.js",
    "build:web": "react-scripts build",
    "lint": "echo 'no linter available'",
    "test": "react-scripts test",
    "clean": "rm -rf ./node_modules ./build ./dist",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@conet.project/seguro-gateway": "0.15.3",
    "@conet.project/seguro-worker-lib": "^0.10.4",
    "@react-icons/all-files": "^4.1.0",
    "@tippyjs/react": "^4.2.6",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "framer-motion": "^7.5.2",
    "get-port": "^6.1.2",
    "react-hot-toast": "^2.4.0",
    "react-loader-spinner": "^5.3.4",
    "react-lottie-player": "^1.4.3",
    "react-router-dom": "^6.4.2",
    "trim-newlines": "^4.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "electron": "^21.1.0",
    "modern-normalize": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-intl": "^6.1.2",
    "react-redux": "^8.0.4",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4",
    "wait-on": "^6.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
