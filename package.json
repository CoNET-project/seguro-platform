{
  "name": "@conet-project/seguro-platform",
  "version": "0.59.1",
  "author": "Kloak Information Technologies",
  "description": "The Seguro Platform application.",
  "license": "UNLICENSED",
  "files": [
    "./build"
  ],
  "main": "./src/electron/electron.js",
  "scripts": {
    "start": "concurrently --kill-others --success=first 'yarn start:gateway' 'yarn _start:web' 'wait-on tcp:3000 && NODE_ENV=development yarn _start:desktop'",
    "start:storybook": "start-storybook -p 6006 -s public",
    "start:gateway": "yarn seguro-gateway --port 3001",
    "start:web": "concurrently --kill-others --success=first 'yarn _start:web'",
    "_start:web": "BROWSER=none react-scripts start",
	"__start:web": "BROWSER=none NODE_ENV=development node src/electron/start.js ",
    "_start:desktop": "electron ./src/electron/electron.js",
    "build:web": "react-scripts build",
    "lint": "echo 'no linter available'",
    "test": "react-scripts test",
    "clean": "rm -rf ./node_modules ./build ./dist",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@conet.project/seguro-gateway": "0.18.3",
    "@conet.project/seguro-worker-lib": "0.13.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@react-icons/all-files": "^4.1.0",
    "@tippyjs/react": "^4.2.6",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.6.1",
    "get-port": "^6.1.2",
    "react-draggable": "^4.4.5",
    "react-hot-toast": "^2.4.0",
    "react-loader-spinner": "^5.3.4",
    "react-lottie-player": "^1.5.4",
    "react-router-dom": "^6.9.0",
    "uuid": "^9.0.0",
	"web-vitals": "^3.3.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "express": "^4.18.2",
	"cors": "^2.8.5",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "electron": "^23.1.4",
    "modern-normalize": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-intl": "^6.3.2",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.9",
    "typescript": "^5.0.2",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
